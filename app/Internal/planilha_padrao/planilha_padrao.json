{
  "openapi": "3.0.2",
  "info": {
    "version": "1.0.0",
    "title": "API interna de planilha padrão",
    "termsOfService": "https://docs.looplex.com.br/legal/termos-de-uso",
    "contact": {
      "name": "Developers Looplex",
      "email": "developer@looplex.com.br",
      "url": "https://www.looplex.com.br"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "x-logo": {
      "url": "https://www.looplex.com.br/img/logo-default.png",
      "href": "/"
    },
    "description": "Documentação de planilha padrão\n"
  },
  "servers": [
    {
      "url": "https://planilhapadrao.lawoffice.com.br",
      "description": "Gerador de arquivo mdb de planilha padrão"
    }
  ],
  "security": [
    {
      "Ocp-Apim-Subscription-Key": []
    }
  ],
  "paths": {
    "/api/generate_mdb_file": {
      "post": {
        "tags": [
          "Planilha Padrão"
        ],
        "summary": "Planilha Padrão",
        "description": "Gerador de arquivo mdb de planilha padrão",
        "operationId": "api-generate_mdb_file-post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ArquivoBase64"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Retorna o base64 do arquivo mdb gerado",
            "content": {
              "application/json": {
                "example": "AAEAAFN0YW5kYXJkIEpldCBEQgABAAAAtW4DYmAJwlXpqWdyQD8AnH6fkP+FmjHFebrtMLzfzJ1j2eTDn0b................"
              }
            }
          },
          "404": {
            "description": "Não encontrado",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ExceptionResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ExceptionResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "api_key": {
        "type": "apiKey",
        "in": "header",
        "name": "api_key"
      },
      "Ocp-Apim-Subscription-Key": {
        "type": "apiKey",
        "in": "header",
        "description": "Subscription key provided by Looplex",
        "name": "Ocp-Apim-Subscription-Key"
      },
      "auth_token": {
        "type": "apiKey",
        "in": "header",
        "name": "auth_token"
      },
      "basic_auth": {
        "type": "http",
        "scheme": "basic"
      }
    },
    "schemas": {
      "ArquivoBase64": {
        "type": "object",
        "properties": {
          "arquivoBase64": {
            "type": "string"
          }
        }
      },
      "ExceptionResponse": {
        "type": "object",
        "properties": {
          "Message": {
            "type": "string",
            "description": "Erro"
          },
          "ExceptionMessage": {
            "type": "string",
            "description": "Descrição do erro"
          }
        }
      }
    }
  }
}